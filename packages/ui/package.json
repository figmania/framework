{
  "name": "@figmania/ui",
  "version": "2.0.11",
  "description": "UI Library for Figmania plugins",
  "main": "build/index.umd.js",
  "module": "build/index.mjs",
  "typings": "build/index.d.ts",
  "license": "MIT",
  "author": "Tobias Strebitzer <tobias.strebitzer@magloft.com>",
  "exports": {
    ".": {
      "import": "./build/index.mjs",
      "require": "./build/index.umd.js"
    },
    "./build/style.css": {
      "import": "./build/style.css",
      "require": "./build/style.css"
    }
  },
  "scripts": {
    "clean": "rimraf build",
    "build": "vite build",
    "watch": "vite build --watch --mode=development",
    "serve": "vite",
    "prepack": "yarn build",
    "lint": "eslint 'src/**/*.ts' 'src/**/*.tsx'"
  },
  "dependencies": {
    "clsx": "^1.2.1",
    "highlight.js": "^11.7.0",
    "html": "^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-outside-click-handler": "^1.3.0",
    "react-scrubber": "^1.1.1"
  },
  "devDependencies": {
    "@figma/plugin-typings": "^1.58.0",
    "@figmania/common": "^2.0.11",
    "@types/highlight.js": "^10.1.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.10",
    "@types/react-highlight": "^0.12.5",
    "@types/react-outside-click-handler": "^1.3.1",
    "@vitejs/plugin-react": "^3.0.0",
    "sass": "^1.58.0",
    "typescript": "^4.9.3",
    "vite": "^4.1.1",
    "vite-plugin-dts": "^1.7.2"
  },
  "peerDependencies": {
    "@figmania/common": "*"
  },
  "resolutions": {
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.10"
  },
  "publishConfig": {
    "access": "public"
  }
}
